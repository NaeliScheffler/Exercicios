#include <stdlib.h>

#include <stdio.h>

#include <locale.h>

int main() {
   setlocale(LC_ALL, "Portuguese");
   int vetor[20], cont = 0, i;

  for (i = 0; i < 20; i++) {
    printf("Insira um elemento inteiro:\n");
    scanf("%d", & vetor[i]);
    if (vetor[i] % 2 == 0) {
    cont++;
}
}
    printf("Quantidade de pares = %d\n", cont);

  return 0;
}
